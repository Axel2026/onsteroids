<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onsteroids">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SilowniaApp">
        <activity android:name="com.example.onsteroids.Cwiczenie"></activity>
        <activity android:name="com.example.onsteroids.PlanTreningu" />
        <activity android:name="com.example.onsteroids.TworzeniePlanu2" />
        <activity
            android:name="com.example.onsteroids.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SilowniaApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.onsteroids.OstatniTrening"
            android:label="@string/app_name"
            android:theme="@style/Theme.SilowniaApp.NoActionBar" />
        <activity
            android:name="com.example.onsteroids.NowyUzytkownik"
            android:label="@string/app_name"
            android:theme="@style/Theme.SilowniaApp.NoActionBar" />
        <activity
            android:name="com.example.onsteroids.TworzeniePlanu"
            android:label="@string/app_name"
            android:theme="@style/Theme.SilowniaApp.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>